<!doctype html>
<html>
<head>
    <title>Download <%- site.title %> for Mac, Windows, Linux, FreeBSD</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <style>
        body {
            padding-top: 80px;
        }
    </style>

    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
    <link rel="stylesheet" href="design/css/layers.min.css" media="screen">
    <link rel="stylesheet" href="design/css/style.css" media="screen">
    <link href='https://fonts.googleapis.com/css?family=Montserrat:400,700|Open+Sans:400italic,700italic,400,700'
          rel='stylesheet' type='text/css'>

    <meta name="description" content="<%- site.large_description %>"/>
    <meta name="keywords" content="<%- site.keywords %>"/>
    <meta name="copyright" content="<%- company.domain %>"/>

    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r;
            i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date();
            a = s.createElement(o),
                m = s.getElementsByTagName(o)[0];
            a.async = 1;
            a.src = g;
            m.parentNode.insertBefore(a, m)
        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');
        ga('create', '<%- site.google_analitics_token %>', 'auto');
        ga('send', 'pageview');
    </script>
</head>
<body>
<div class="container">
    <div class="well">
        <h3>
            <p>Download <%= site.small_description %></p>
            <p>The latest stable version:</p>
        </h3>
        <div class="row">
            <div class="row">
                <div class="col-md-12">
                    <h3>
                        <img src="/images/macosx.png" alt="" title="">
                        <%= project.name %> <%= project.version %> <span class="dowm-os">For Mac OS X:</span>
                    </h3>
                </div>
            </div>
            <!-- version -->
            <p>
                <a href="/downloads/macosx/<%= project.name_lowercase %>-<%= project.version %>-x86_64.dmg">
                    64 bit Mac OS X <%= project.version %> Installer (.dmg)
                </a>
            </p>
            <p>
                <a href="/downloads/macosx/<%= project.name_lowercase %>-<%= project.version %>-x86_64.zip">
                    Application Archive <%= project.version %> (.zip)
                </a>
            <p>
        </div>

        <div class="row">
            <div class="row">
                <div class="col-md-12">
                    <h3>
                        <img src="/images/windows.png" alt="" title="">
                        <%= project.name %> <%= project.version %> <span class="dowm-os">For Windows:</span>
                    </h3>
                </div>
            </div>
            <!-- version -->
            <p>
                <a href="/downloads/windows/<%= project.name_lowercase %>-<%= project.version %>-x86_64.exe">
                    64 bit Windows <%= project.version %> Installer (.exe)
                </a>
            </p>
            <p>
                <a href="/downloads/windows/<%= project.name_lowercase %>-<%= project.version %>-x86_64.zip">
                    64 bit Application Archive <%= project.version %> (.zip)
                </a>
            </p>
        </div>

        <div class="row">
            <div class="row">
                <div class="col-md-12">
                    <h3>
                        <img src="/images/linux.png" alt="" title="">
                        <%= project.name %> <%= project.version %> <span class="dowm-os">For Linux:</span>
                    </h3>
                </div>
            </div>
            <!-- version -->
            <p>
                <a href="/downloads/linux/<%= project.name_lowercase %>-<%= project.version %>-x86_64.deb">
                    Debian/Ubuntu <%= project.version %> Package (.deb)
                </a>
            </p>
            <p>
                <a href="/downloads/linux/<%= project.name_lowercase %>-<%= project.version %>-x86_64.rpm">
                    Fedora/openSUSE <%= project.version %> Package (.rpm)
                </a>
            </p>
            <p>
                <a href="/downloads/linux/<%= project.name_lowercase %>-<%= project.version %>-x86_64.tar.gz">
                    Application Archive <%= project.version %> (.tar.gz)
                </a>
            <p>
        </div>

        <div class="row">
            <div class="row">
                <div class="col-md-12">
                    <h3>
                        <img src="/images/android.png" alt="" title="">
                        <%= project.name %> <%= project.version %> <span class="dowm-os">For Android:</span>
                    </h3>
                </div>
            </div>
            <!-- version -->
            <p>
                <a href="/downloads/android/<%= project.name_lowercase %>-<%= project.version %>-armeabi-v7a.apk">
                    Android <%= project.version %> Package (.apk)
                </a>
            </p>
        </div>

        <div class="row">
            <div class="row">
                <div class="col-md-12">
                    <h3>
                        <img src="/images/freebsd.png" alt="" title="">
                        <%= project.name %> <%= project.version %> <span class="dowm-os">For FreeBSD:</span>
                    </h3>
                </div>
            </div>
            <!-- version -->
            <p>
                <a href="/downloads/freebsd/<%= project.name_lowercase %>-<%= project.version %>-x86_64.tar.gz">
                    FreeBSD <%= project.version %> Package (.tar.gz)
                </a>
            </p>
        </div>

    </div>
</div>
</body>
</html>
